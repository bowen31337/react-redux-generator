import * as actions from '../actions'
import * as types from '../constants'

describe('EntryIvcForm actions', () => {
	it('Fetch Remote Static Data action', () => {
		expect(actions.fetchRemoteData()).toEqual({
			type: types.FETCH_REMOTE_DATA,
		})
	})
	it('Successfully Fetch Remote Static Data action', () => {
		expect(actions.fetchRemoteDataSuccess({})).toEqual({
			type: types.FETCH_REMOTE_DATA_SUCCESS,
			payload: {},
		})
	})
	it('Failed to Fetch Remote Static Data action', () => {
		expect(actions.fetchRemoteDataFailure({})).toEqual({
			type: types.FETCH_REMOTE_DATA_FAILURE,
			payload: {},
		})
	})
})
